[SERVICE]
    Flush        5
    Daemon       Off
    Log_Level    info

[INPUT]
    Name        kafka
    Brokers     <%= broker %>
    Topics      <%= topics %>
    Group_Id    <%= groupId %>
    Tag         <%= kafkaTag %>

[OUTPUT]
    Name        es
    Match       <%= matchTag %>
    Host        <%= host %>
    Port        <%= port %>
    Index       <%= esIndex %>
    Type        _doc
    HTTP_User   <%= username || "" %>
    HTTP_Passwd <%= password || "" %>
    tls         <%= tls %>
    tls.verify  <%= tls && skipTlsVerify ? "Off" : "On" %>
